{
  "name": "n8n-nodes-datastore-pg",
  "version": "0.1.18",
  "description": "Datastore for n8n with PostgreSQL support - store data in memory or PostgreSQL database",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Dmitrii Korotovskii",
    "email": "dmitry@korotovsky.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/korotovsky/n8n-nodes-datastore-pg.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Datastore/Datastore.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "pg": "^8.13.1"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
